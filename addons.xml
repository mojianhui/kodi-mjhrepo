		<import addon="xbmc.gui" version="5.3.0"/>
		<import addon="service.skin.widgets" version="0.0.29"/>
		<import addon="script.tvtunes" version="3.0.6"/>
		<import addon="script.tv.show.next.aired" version="6.0.10"/>
		<import addon="script.artwork.downloader" version="12.0.24"/>
		<import addon="script.globalsearch" version="2.0.8"/>
		<import addon="script.artistslideshow" version="1.6.5"/>
		<import addon="script.playalbum" version="1.0.1"/>
		<import addon="script.extendedinfo" version="1.0.0"/>
		<import addon="script.grab.fanart" version="0.12.8"/>
		<import addon="script.skinshortcuts" version="0.4.4"/>
	</requires>
	<extension point="xbmc.gui.skin" defaultthemename="Textures.xbt" effectslowdown="0.75" debugging="false">
	    <res width="1280" height="720" aspect="16:9" default="true" folder="720p"/>
	</extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">A GUI for Kodi Media Center</summary>
    <summary lang="zh">一个 Kodi 多媒体中心的图形界面</summary>
    <summary lang="zh_TW">一個 Kodi 多媒體中心的圖形介面</summary>
    <description lang="en">Bello is a GUI for Kodi Media Center. The goal of Bello is to maximize your Kodi entertainment experience with it's fast, unique and unified interface along with all the latest features of Kodi.</description>
	<description lang="zh">重新编译更换了字体文件,支持中文显示</description>
	<description lang="zh">重新編譯更換了字體文件,支持中文顯示</description>
    <disclaimer lang="en">Can not support mouse</disclaimer>
    <disclaimer lang="zh">不支持使用鼠标</disclaimer>
    <disclaimer lang="zh_TW">不支援使用滑鼠</disclaimer>
    <platform>all</platform>
    <license>Creative Commons Attribution-NonCommercial-Share Alike 4.0 International Public License</license>
    <forum>http://forum.xbmc.org/showthread.php?tid=158577</forum>
    <email>nessus85100 at hotmail dot com</email>
    <source>https://github.com/Nessus85100/Bello</source>
  </extension>
</addon>
